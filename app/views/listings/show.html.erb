<div class="jumbotron" style="background-image: url(https://resources.vrbo.com/resources/44226/images/banner-438967.jpg); background-size: 100%;">
   <div class="containerforabout">
   <h1>Available Listings</h1>
   </div>
</div>

<div class="row">
  <div class="col-md-6 col-md-offset-3">.col-md-6 .col-md-offset-3</div>
    <table class="table table-striped">
        <
    </table>
</div>



  <div class="data col-sm-4">
    <div>
    <div id ="listing-pictures">
        
          <%= image_tag @listing.image_url.to_s %>
        </div>
      <div class="details primary">
        <h4 itemprop="name"><%=@listing.description%></h4>
        <h6>Uploaded By: <%=@listing.user.first_name%></h6>
        <p>Listing City: <%=@listing.city%></p>
        <p>Address: <%=@listing.address%></p>
        <p>Accommodates: <%=@listing.accomodates%></p>
      </div>
      <div class="pricing secondary">
        <p class="currency">MYR <%=@listing.price%></p>
        <p class="per-night">per night</p>
          <button class="btn btn-sm btn-deal"><span class="instant-booking-icon"></span>Book Now!</button>
      </div>
              <div class="actions">
        <%= link_to "edit", edit_listing_path(@listing) %> |
        <%= link_to "Delete", listing_path(@listing), :confirm => "Are you sure?", :method => :delete %>
      </div>
    </div>

<!-- Extra small button group -->
<div class="btn-group col-sm-4">
  <button class="btn btn-default btn-xs dropdown-toggle" type="button" data-toggle="dropdown" aria-haspopup="true" aria-expanded="false">
    More Details <span class="caret"></span>
  </button>
  <ul class="dropdown-menu">
    <li>Smoking Room: <%=@listing.smoking_room%> </li>
    <li>Pet-friendly: <%=@listing.pet_friendly%> </li>
  </ul>
</div>




